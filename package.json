{
  "name": "crawler-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.config.js --testMatch '**/e2e/**/*.test.ts'",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up",
    "docker:start:detach": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:down:volumes": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "docker:clean": "docker-compose down -v --rmi all",
    "docker:restart": "npm run docker:stop && npm run docker:start",
    "docker:rebuild": "npm run docker:down && npm run docker:build && npm run docker:start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.47.7",
    "axios": "^1.7.9",
    "bullmq": "^5.34.1",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "nock": "^13.5.6",
    "robots-parser": "^3.0.1",
    "supertest": "^7.0.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5"
  }
}
